using System;

class MainClass {
  public static void Main (string[] args) {
    int a, b ,c, ab, maior;
    string[] valores = Console.ReadLine().Split(' ');
    a = int.Parse(valores[0]);
    b = int.Parse(valores[1]);
    c = int.Parse(valores[2]);
    ab = Math.Max(a,b);
    maior = Math.Max(ab,c);
    Console.WriteLine($"{maior} eh o maior");
    
  }
}
